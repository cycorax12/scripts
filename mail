routes:
  - name: depen-explorer-proxy
    uris:
      - /depen-explorer/*
    vars:
      - ["uri", "~", "^/depen-explorer/(.*)"]
    plugins:
      proxy-rewrite:
        regex_uri:
          - "^/depen-explorer/(.*)"
          - "/$1"
    upstream:
      type: roundrobin
      scheme: https
      pass_host: rewrite
      upstream_host: ymjost.com
      nodes:
        - host: ymjost.com
          port: 443
          weight: 1
    status: 1
