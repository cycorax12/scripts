SELECT TO_CHAR(LAST_UPDATED_TIME, 'DD-MON-YYYY HH24:MI:SS.FF3') AS ts_millis,
       COUNT(*) AS row_count
FROM settlement_detail
WHERE LAST_UPDATED_TIME >= TO_TIMESTAMP('25-JUL-2025 04:23:00', 'DD-MON-YYYY HH24:MI:SS')
  AND LAST_UPDATED_TIME <  TO_TIMESTAMP('25-JUL-2025 04:24:00', 'DD-MON-YYYY HH24:MI:SS')
GROUP BY TO_CHAR(LAST_UPDATED_TIME, 'DD-MON-YYYY HH24:MI:SS.FF3')
ORDER BY row_count DESC;
